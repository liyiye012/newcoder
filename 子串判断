class Substr {
public:
    vector<bool> chkSubStr(vector<string> p, int n, string s) {
        // write code here
            vector<bool> res;
    for (int i = 0; i < n; i++) {
        res.push_back(s.find(p[i]) != string::npos);
    }
    //for (int j = 0; j < res.size(); j++) {
    //  cout << res[j] << " ";
    //}
    return res;
    }
};
