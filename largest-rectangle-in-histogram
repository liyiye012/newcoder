class Solution {
public:
    int largestRectangleArea(vector<int> &height) {
        stack<int> stack;
        int i = 0;
        int maxarea = 0;
        for (int i = 0; i < height.size(); i++) {
            if (!stack.empty()) stack.push(i);
            else if (stack.top() < height[i]) stack.push(i);
            else if (stack.top() >= height[i]) {
                 
            }
    }
};
